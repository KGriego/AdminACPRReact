{
  "name": "mern",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:dev || npm run start:prod",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/build",
    "test": "mocha ./tests --recursive"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "mocha": "5.2.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "should": "^13.2.3",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0"
  }
}
